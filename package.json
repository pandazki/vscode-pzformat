{
  "name": "pzformat",
  "displayName": "pzformat",
  "description": "a clipboard format util.",
  "version": "1.0.0",
  "publisher": "pandazki",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.pzformat"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.pzformat",
        "title": "Clipboard Format"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "sprintf-js": "^1.1.1"
  }
}
